package com.latihan;
public class Karyawan {


    String NIP;
    String nama;
    String jenisKelamin;

    public void masukKerja() {
        System.out.println("Masuk kerja");
    }

    public void beriNama(String nama) {
        this.nama = nama;
    }
}

package com.latihan;

public class Main {

    public static void main(String[] args) {
        Dosen dosen = new Dosen();
        dosen.masukKerja();
    }

}
